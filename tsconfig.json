{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "types": ["miniprogram-api-typings", "node"],
    "outDir": "./lib",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "removeComments": false,
    "declaration": true,
    "sourceMap": true,
    "skipLibCheck": true
  },
  "include": ["./src"]
}
